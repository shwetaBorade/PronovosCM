<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="PDFTronThemeBase" parent="Theme.MaterialComponents.Light.NoActionBar.Bridge">
    </style>

    <style name="PDFTronThemeCommon" parent="PDFTronThemeBase">
        <!-- Default -->
        <item name="actionBarPopupTheme">@style/AppTheme.PopupOverlay</item>
        <item name="actionModeBackground">?attr/colorPrimary</item>
        <item name="windowActionModeOverlay">true</item>
        <!-- Drawer -->
        <item name="drawerArrowStyle">@style/DrawerArrowStyle</item>
    </style>

    <!-- New UI -->
    <style name="PDFTronAppThemeBase" parent="PDFTronThemeCommon">
        <!-- Toolbar -->
        <item name="pt_toolbar_style">@style/ToolbarStyle</item>
        <item name="pt_toolbar_theme">@style/ToolbarTheme</item>
        <item name="pt_toolbar_popup_theme">@style/ToolbarPopupTheme</item>
        <item name="toolbarNavigationButtonStyle">@style/ToolbarButtonNavigationStyle</item>

        <!-- Document Tab Layout -->
        <item name="pt_pdf_tab_layout_style">@style/PdfTabLayoutStyle</item>

        <!-- Action bar -->
        <item name="actionMenuTextColor">@color/toolbar_icon</item>
        <item name="actionModeCloseButtonStyle">@style/ActionModeCloseButtonStyle</item>
        <item name="actionOverflowButtonStyle">@style/ActionModeOverflowButtonStyle</item>

        <!-- Signature / stamp tab -->
        <item name="pt_stamp_tab_layout_style">@style/StampTabLayoutStyle</item>
        <!-- Bookmarks tab -->
        <item name="custom_bookmarks_tab_layout">@style/BookmarksTabLayoutStyle</item>
        <!-- Navigation FAB -->
        <item name="pt_floating_nav_style">@style/DefaultFloatingButtonNavStyle</item>
        <!--Sound dialog-->
        <item name="pt_sound_dialog_style">@style/SoundDialogStyle</item>
    </style>

    <style name="PDFTronAppTheme" parent="PDFTronAppThemeBase">
        <item name="colorPrimary">@color/pdftron_color_primary_day</item>
        <item name="colorPrimaryDark">@color/pdftron_color_primary_dark_day</item>
        <item name="colorAccent">@color/app_color_accent</item>
        <item name="android:colorBackground">@color/pdftron_color_primary_day</item>
    </style>

    <!-- Legacy UI -->
    <style name="CustomAppThemeBase" parent="PDFTronThemeCommon">
        <!-- Toolbar -->
        <item name="pt_toolbar_style">@style/ToolbarLegacyStyle</item>
        <item name="pt_toolbar_theme">@style/ToolbarLegacyTheme</item>
        <item name="pt_toolbar_popup_theme">@style/ToolbarPopupTheme</item>
        <item name="toolbarNavigationButtonStyle">@style/ToolbarButtonNavigationLegacyStyle</item>

        <!-- Action bar -->
        <item name="actionMenuTextColor">@color/app_color_icon</item>
        <item name="actionModeCloseButtonStyle">@style/ActionModeCloseButtonLegacyStyle</item>
        <item name="actionOverflowButtonStyle">@style/ActionModeOverflowButtonLegacyStyle</item>

        <!-- Annotation toolbar -->
        <item name="annotation_toolbar">@style/CustomAnnotationToolbarLegacyStyle</item>

        <!-- Signature / stamp tab -->
        <item name="pt_stamp_tab_layout_style">@style/StampTabLayoutLegacyStyle</item>
        <!-- Bookmarks tab -->
        <item name="custom_bookmarks_tab_layout">@style/BookmarksTabLayoutLegacyStyle</item>
        <!-- Navigation FAB -->
        <item name="pt_floating_nav_style">@style/DefaultFloatingButtonNavStyle</item>
        <!--Sound dialog-->
        <item name="pt_sound_dialog_style">@style/SoundDialogLegacyStyle</item>
    </style>

    <style name="CustomAppTheme" parent="CustomAppThemeBase">
        <item name="colorPrimary">@color/app_color_primary_day</item>
        <item name="colorPrimaryDark">@color/app_color_primary_dark_day</item>
        <item name="colorAccent">@color/app_color_accent_legacy</item>
    </style>

    <!--  New UI components START -->

    <style name="ToolbarStyle">
        <item name="android:background">?attr/colorPrimary</item>
    </style>

    <style name="ToolbarTheme" parent="ThemeOverlay.AppCompat.Dark.ActionBar">
        <item name="titleTextColor">@color/toolbar_icon</item>
        <item name="colorControlNormal">@color/toolbar_icon</item>
        <item name="iconTint">@color/toolbar_icon</item>
        <!--Search-->
        <item name="android:editTextColor">@color/toolbar_icon</item>
        <item name="android:textColorHint">@color/toolbar_icon</item>
    </style>

    <style name="ActionModeCloseButtonStyle" parent="Widget.AppCompat.ActionButton.CloseMode">
        <item name="android:tint">@color/toolbar_icon</item>
    </style>

    <style name="ActionModeOverflowButtonStyle" parent="Widget.AppCompat.ActionButton.Overflow">
        <item name="android:tint">@color/toolbar_icon</item>
    </style>

    <style name="ToolbarButtonNavigationStyle" parent="Widget.AppCompat.Toolbar.Button.Navigation">
        <item name="android:tint">@color/toolbar_icon</item>
    </style>

    <style name="StampTabLayoutStyle" parent="BaseStampTabLayoutStyle">
        <item name="android:background">@color/annot_toolbar_background_primary</item>
        <item name="tabTextColor">@color/annot_toolbar_icon</item>
        <item name="tabSelectedTextColor">@color/annot_toolbar_icon</item>
        <item name="tabIconTint">@color/annot_toolbar_icon</item>
    </style>

    <style name="BookmarksTabLayoutStyle" parent="BaseBookmarksTabLayoutStyle">
        <item name="tabBackgroundSheet">@color/annot_toolbar_background_primary</item>
        <item name="tabBackgroundDialog">@color/annot_toolbar_background_primary</item>
        <item name="tabTintColorSheet">@color/annot_toolbar_icon</item>
        <item name="tabTintColorDialog">@color/annot_toolbar_icon</item>
        <item name="tabTintSelectedColorDialog">?attr/colorAccent</item>
        <item name="tabTintSelectedColorSheet">?attr/colorAccent</item>
    </style>

    <style name="DefaultFloatingButtonNavStyle">
        <item name="iconTint">@color/pt_secondary_color</item>
        <item name="backgroundTint">@color/pt_background_color</item>
        <item name="dividerColor">@color/pt_subtle_utility_color</item>
        <item name="disabledIconColor">@color/pt_disabled_state_color</item>
    </style>

    <style name="SoundDialogStyle">
        <item name="iconTint">@color/annot_toolbar_icon</item>
    </style>

    <style name="SoundAnnotViewTheme">
        <item name="iconColor">@color/pt_body_text_color</item>
        <item name="disabledIconColor">@color/pt_disabled_state_color</item>
        <item name="secondaryTextColor">@color/pt_secondary_color</item>
    </style>

    <!--  New UI components END -->

    <!--  Legacy UI components START -->

    <style name="ToolbarLegacyStyle">
        <item name="android:background">?attr/colorPrimary</item>
    </style>

    <style name="ToolbarLegacyTheme" parent="ThemeOverlay.AppCompat.Dark.ActionBar">
        <item name="titleTextColor">@color/app_color_icon</item>
        <item name="colorControlNormal">@color/app_color_icon</item>
        <item name="iconTint">@color/app_color_icon</item>
        <!--Search-->
        <item name="android:editTextColor">@android:color/white</item>
        <item name="android:textColorHint">@android:color/white</item>
    </style>

    <style name="ActionModeCloseButtonLegacyStyle" parent="Widget.AppCompat.ActionButton.CloseMode">
        <item name="android:tint">@color/app_color_icon</item>
    </style>

    <style name="ActionModeOverflowButtonLegacyStyle" parent="Widget.AppCompat.ActionButton.Overflow">
        <item name="android:tint">@color/app_color_icon</item>
    </style>

    <style name="ToolbarButtonNavigationLegacyStyle" parent="Widget.AppCompat.Toolbar.Button.Navigation">
        <item name="android:tint">@color/app_color_icon</item>
    </style>

    <style name="CustomAnnotationToolbarLegacyStyle" parent="AnnotationToolbarStyle">
        <item name="colorToolIcon">@color/app_color_icon</item>
        <item name="colorCloseIcon">@color/app_color_icon</item>
    </style>

    <style name="StampTabLayoutLegacyStyle" parent="BaseStampTabLayoutStyle">
        <item name="tabTextColor">@color/app_color_icon</item>
        <item name="tabSelectedTextColor">@color/app_color_icon</item>
        <item name="tabIconTint">@color/app_color_icon</item>
    </style>

    <style name="BookmarksTabLayoutLegacyStyle" parent="BaseBookmarksTabLayoutStyle">
        <item name="tabBackgroundSheet">?android:attr/colorBackground</item>
        <item name="tabBackgroundDialog">?attr/colorPrimary</item>
        <item name="tabTintColorSheet">@color/navigation_list_title_color</item>
        <item name="tabTintColorDialog">@color/app_color_icon</item>
        <item name="tabTintSelectedColorDialog">@color/app_color_icon</item>
        <item name="tabTintSelectedColorSheet">@color/navigation_list_title_color</item>
    </style>

    <style name="SoundDialogLegacyStyle">
        <item name="iconTint">?attr/colorPrimary</item>
    </style>

    <!--  Legacy UI components END -->

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="DrawerArrowStyle" parent="Widget.AppCompat.DrawerArrowToggle">
        <item name="spinBars">true</item>
        <item name="color">@android:color/white</item>
    </style>

    <style name="ToolbarPopupTheme" parent="ThemeOverlay.AppCompat.Light" />

    <style name="transparent_button" parent="Widget.AppCompat.Button.Borderless">
        <item name="android:minWidth">@dimen/dialog_button_min_width</item>
        <item name="android:paddingLeft">@dimen/dialog_button_default_margin_padding</item>
        <item name="android:paddingRight">@dimen/dialog_button_default_margin_padding</item>
    </style>

    <style name="transparent_dialog_annotation_note_button" parent="Widget.AppCompat.Button.Borderless">
        <item name="android:minWidth">@dimen/dialog_button_min_width</item>
        <item name="android:paddingLeft">@dimen/dialog_button_default_margin_padding</item>
        <item name="android:paddingRight">@dimen/dialog_button_default_margin_padding</item>
        <item name="android:textColor">@color/button_dialog_annotation_note_color</item>
    </style>

    <style name="black_text_button" parent="Widget.AppCompat.Button.Borderless">
        <item name="android:textColor">@color/button_text_color</item>
    </style>

    <style name="fabMenuLabelStyle">
        <item name="android:background">@drawable/fab_menu_label_background</item>
        <item name="android:textColor">@android:color/white</item>
    </style>

    <style name="StatusBarView">
        <!-- Fit system window insets by default, since the StatusBarView widget is
             only really useful when the fitsSystemWindows flag is set. -->
        <item name="android:fitsSystemWindows">true</item>
        <item name="android:background">?attr/colorPrimaryDark</item>
    </style>

    <style name="DispatchFairInsets">
        <!-- Fully consume the system window insets by default. -->
        <item name="consumeInsets">true</item>
    </style>

    <style name="DispatchFairInsetsRelativeLayout" parent="DispatchFairInsets" />

    <style name="DispatchFairInsetsFrameLayout" parent="DispatchFairInsets" />

    <style name="DispatchFairInsetsLinearLayout" parent="DispatchFairInsets" />

    <style name="BaseDocumentSliderStyle">
        <item name="seekbarColor">@color/document_slider_chip_tint</item>
        <item name="colorBackground">@color/document_slider_chip_background</item>
    </style>

    <style name="DocumentSliderStyle" parent="BaseDocumentSliderStyle">
    </style>

    <style name="BaseThumbnailSliderStyle">
        <!-- Set the description of the menu button left of the slider -->
        <item name="leftMenuItemContentDescription">@string/pref_viewmode_thumbnails</item>
        <!-- Set the color of the menu button right of the slider -->
        <item name="rightMenuItemContentDescription">@string/action_outline</item>

        <!-- Set the icon of the menu button left of the slider -->
        <item name="leftMenuItemDrawable">@drawable/ic_thumbnails_grid_black_24dp</item>
        <!-- Set the icon of the menu button right of the slider -->
        <item name="rightMenuItemDrawable">@drawable/ic_outline_white_24dp</item>
    </style>

    <style name="ThumbnailSliderStyle" parent="BaseThumbnailSliderStyle">
        <item name="leftMenuItemColor">?attr/colorPrimary</item>
        <item name="rightMenuItemColor">?attr/colorPrimary</item>
        <item name="seekbarColor">?attr/colorPrimary</item>
    </style>

    <style name="ThumbnailSliderStyle.Seekbar" parent="Widget.AppCompat.SeekBar" >
        <item name="android:paddingTop">16dp</item>
        <item name="android:paddingBottom">16dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:minHeight">2dp</item>
        <item name="android:maxHeight">2dp</item>
    </style>

    <style name="BaseThumbnailBrowserStyle">
        <item name="showFilterMenuItem">true</item>
        <item name="showFilterAnnotated">true</item>
        <item name="showFilterBookmarked">true</item>
        <item name="showAddBookmarkMenuItem">true</item>
    </style>

    <style name="ThumbnailBrowserStyle" parent="BaseThumbnailBrowserStyle">
    </style>

    <style name="BaseFloatingActionMenuStyle">
        <item name="menu_colorNormal">?attr/colorAccent</item>
        <item name="menu_colorPressed">?attr/colorAccent</item>
        <item name="android:tint">@color/tools_colors_white</item>
    </style>

    <style name="FloatingActionMenuStyle" parent="BaseFloatingActionMenuStyle">
    </style>

    <style name="BaseFloatingActionButtonStyle">
        <item name="fab_colorNormal">?attr/colorAccent</item>
        <item name="fab_colorPressed">?attr/colorAccent</item>
        <item name="android:tint">@color/tools_colors_white</item>
    </style>

    <style name="FloatingActionButtonStyle" parent="BaseFloatingActionButtonStyle">
    </style>

    <style name="BaseFloatingButtonNavStyle" parent="Widget.Design.FloatingActionButton">
        <item name="elevation">@dimen/elevation_low</item>
        <item name="fabCustomSize">@dimen/page_jump_button_size</item>
        <item name="backgroundTint">@color/page_number_indicator_bg</item>
        <item name="android:layout_gravity">center</item>
    </style>

    <style name="FloatingButtonNavStyle" parent="BaseFloatingButtonNavStyle">
        <item name="tint">@color/document_fab_tint</item>
    </style>

    <style name="BaseRotateButtonStyle" parent="Widget.Design.FloatingActionButton">
        <item name="elevation">@dimen/elevation_low</item>
        <item name="fabCustomSize">@dimen/rotate_button_size</item>
        <item name="android:layout_gravity">center</item>
        <item name="backgroundTint">@color/rotation_handle_bg</item>
    </style>

    <style name="RotateButtonStyle" parent="BaseRotateButtonStyle">
        <item name="tint">@color/tools_selection_control_point</item>
    </style>

    <style name="SelectionHandleStyle" parent="BaseRotateButtonStyle">
        <item name="backgroundTint">@color/tools_selection_control_point</item>
        <item name="tint">@color/rotation_handle_bg</item>
        <item name="maxImageSize">@dimen/selection_widget_size</item>
    </style>

    <style name="BasePdfTabLayoutStyle" parent="Widget.Design.TabLayout">
        <item name="tabContentStart">@dimen/first_keyline</item>
        <item name="tabPaddingEnd">0dp</item>
        <item name="tabPaddingStart">0dp</item>
        <item name="tabTextAppearance">@style/PdfTabTextAppearance</item>
    </style>

    <style name="PdfTabTextAppearance" parent="TextAppearance.Design.Tab">
        <item name="android:textColor">@color/selector_tab_color_fg</item>
    </style>

    <style name="PdfTabLayoutStyle" parent="BasePdfTabLayoutStyle">
        <item name="android:background">?attr/colorPrimary</item>
        <item name="tabSelectedTextColor">@color/annot_toolbar_selected_icon</item>
        <item name="tabTextColor">@color/annot_toolbar_selected_icon</item>
    </style>

    <style name="BaseBookmarksTabLayoutStyle" parent="Widget.Design.TabLayout">
        <item name="tabMaxWidth">0dp</item>
        <item name="tabGravity">fill</item>
        <item name="tabMode">fixed</item>
    </style>

    <style name="BaseStampTabLayoutStyle" parent="Widget.Design.TabLayout">
        <item name="android:background">?attr/colorPrimary</item>
        <item name="tabGravity">fill</item>
        <item name="tabMaxWidth">0dp</item>
        <item name="tabMode">fixed</item>
        <item name="tabSelectedTextColor">?android:attr/textColorPrimaryInverse</item>
        <item name="tabTextColor">@android:color/primary_text_dark</item>
    </style>

    <style name="AnnotationToolbarStyle">
        <item name="colorBackground">?attr/colorPrimary</item>
        <item name="colorToolBackground">?attr/colorPrimaryDark</item>
        <item name="colorToolIcon">@android:color/white</item>
        <item name="colorCloseIcon">@android:color/white</item>
    </style>

    <style name="EditToolbarStyle">
        <item name="colorBackground">?android:attr/colorBackground</item>
        <item name="colorToolBackground">@color/controls_edit_toolbar_tool</item>
        <item name="colorToolIcon">?android:attr/colorForeground</item>
        <item name="colorCloseIcon">@color/controls_edit_toolbar_checkmark</item>
    </style>

    <style name="FormToolbarStyle" parent="AnnotationToolbarStyle">
    </style>

    <style name="BaseSearchToolbarStyle">
        <item name="showListAll">true</item>
        <item name="showMatchCase">true</item>
        <item name="showWholeWord">true</item>
        <item name="showSearchWeb">true</item>
    </style>

    <style name="SearchToolbarStyle" parent="BaseSearchToolbarStyle">
    </style>

    <style name="BaseDialogStickyNoteStyle">
        <item name="showStyleIcon">true</item>
        <!-- If true, then the edit text background color will follow editTextBackgroundColor. Otherwise
             it will follow the sticky note color (light mode only). -->
        <item name="forceEditTextBackgroundColor">false</item>
    </style>

    <style name="DialogStickyNoteStyle" parent="BaseDialogStickyNoteStyle">
    </style>

    <style name="DialogAnnotNoteStyle">
        <item name="backgroundColor">@color/tools_dialog_annotation_popup_textbox_background</item>
        <item name="editTextBackgroundColor">@color/tools_dialog_annotation_popup_background</item>
        <item name="textButtonColor">@color/button_dialog_annotation_note_color</item>
    </style>

    <style name="QuickMenuButton" parent="Widget.AppCompat.Button.Borderless">
        <item name="android:background">?attr/selectableItemBackground</item>
    </style>

    <style name="RectGroupAnnotEdit">
        <item name="fillColor">@color/tools_annot_edit_line_shadow</item>
        <item name="fillOpacity">0.38</item>
        <item name="lassoColor">@color/tools_annot_edit_line_shadow</item>
        <item name="lassoOpacity">1</item>
        <item name="lassoThickness">2</item>
    </style>

    <style name="TabFragmentToolManager">
        <item name="edit_ink_annots">true</item>
        <item name="add_image_stamper_tool">true</item>
        <item name="open_toolbar_on_pan_ink_selected">true</item>
        <item name="build_in_page_number_indicator">false</item>
    </style>

    <style name="PresetColorGridViewStyle">
        <item name="color_list">@array/preset_colors</item>
        <item name="highlight_color_list">@array/preset_highlight_colors</item>
        <item name="android:horizontalSpacing">@dimen/padding_xsmall</item>
        <item name="android:verticalSpacing">@dimen/padding_xsmall</item>
        <item name="android:stretchMode">columnWidth</item>
        <item name="android:numColumns">7</item>
        <item name="android:clipToPadding">false</item>
    </style>

    <style name="PresetStyleGridViewStyle">
        <item name="android:horizontalSpacing">0dp</item>
        <item name="android:verticalSpacing">0dp</item>
        <item name="android:stretchMode">columnWidth</item>
        <item name="android:numColumns">5</item>
        <item name="android:clipToPadding">false</item>
    </style>

    <style name="ColorPickerStyle">
        <item name="wheel_width">@dimen/quick_menu_button_size</item>
        <item name="value_pointer_radius">@dimen/padding_large</item>
        <item name="ring_color">@android:color/white</item>
        <item name="ring_shadow_radius">@dimen/action_button_min_width</item>
    </style>

    <style name="AnnotStyleBottomSheetDialogStyle" parent="Theme.Design.BottomSheetDialog">
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
    </style>

    <style name="FullScreenDialogStyle" parent="ThemeOverlay.AppCompat.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="ToolbarSwitcherDialogStyle" parent="FullScreenDialogStyle">
        <item name="android:backgroundDimEnabled">@bool/isPhone</item>
    </style>

    <style name="SignatureSelectionDialogStyle" parent="FullScreenDialogStyle">
        <item name="android:backgroundDimEnabled">false</item>
    </style>

