    <declare-styleable name="ToolManager">
        <!-- whether editing ink annotation should open the annotation toolbar -->
        <attr name="edit_ink_annots" format="boolean" />
        <!-- whether to enable image stamper tool -->
        <attr name="add_image_stamper_tool" format="boolean" />
        <!-- whether ink tool will open annotation toolbar -->
        <attr name="open_toolbar_on_pan_ink_selected" format="boolean" />
        <!-- whether to use/show the built-in page number indicator -->
        <attr name="build_in_page_number_indicator" format="boolean" />
        <!-- whether to check annotation author permission -->
        <attr name="annot_permission_check" format="boolean" />
        <!-- whether to show author dialog the first time when user annotates -->
        <attr name="show_author_dialog" format="boolean" />
        <!-- whether the TextMarkup annotations are compatible with Adobe
        (Adobe's quads don't follow the specification, but they don't handle quads that do)
        -->
        <attr name="text_markup_adobe_hack" format="boolean" />
        <!-- whether to copy annotated text to note -->
        <attr name="copy_annotated_text_to_note" format="boolean" />
        <!-- whether to use stylus to draw without entering ink tool -->
        <attr name="stylus_as_pen" format="boolean" />
        <!-- whether to smooth ink annotation -->
        <attr name="ink_smoothing_enabled" format="boolean" />
        <!-- whether auto select annotation after annotation is created -->
        <attr name="auto_select_annotation" format="boolean" />
        <!-- whether disable showing quick menu -->
        <attr name="quick_menu_disable" format="boolean" />
        <!-- whether can double tap to zoom -->
        <attr name="double_tap_to_zoom" format="boolean" />
        <!-- whether can auto resize free text when editing -->
        <attr name="auto_resize_freetext" format="boolean" />
        <!-- whether annotation editing is real time -->
        <attr name="realtime_annot_edit" format="boolean" />
        <!-- whether can edit freetext on tap -->
        <attr name="edit_freetext_on_tap" format="boolean" />
        <!-- whether to show the freetext inline toggle button -->
        <attr name="freeText_inline_toggle_enabled" format="boolean" />
        <!-- whether to show the rich content switch for freetext -->
        <attr name="freeText_show_rich_content_switch" format="boolean" />
        <!-- whether can show saved signatures in signature dialog -->
        <attr name="show_saved_signatures" format="boolean" />
        <!-- whether "Store signature" setting in the CreateSignatureFragment dialog should be enabled or disabled by default. -->
        <attr name="default_store_new_signature" format="boolean" />
        <!-- whether to use the user's last set "Store signature" setting in the CreateSignatureFragment dialog.-->
        <attr name="persist_store_signature_setting" format="boolean" />
        <!-- whether can signature presets are shown in signature dialog -->
        <attr name="show_signature_presets" format="boolean" />
        <!-- whether can show pick from image button in signature dialog -->
        <attr name="show_signature_from_image" format="boolean" />
        <!-- whether can show the typed signature button in signature dialog -->
        <attr name="show_typed_signature" format="boolean" />
        <!-- whether can show indicator for annotations with comments -->
        <attr name="show_annot_indicator" format="boolean" />
        <!-- whether has separate annotation layer -->
        <attr name="annotation_layer_enabled" format="boolean" />
        <!-- whether use digital signature when signing -->
        <attr name="use_digital_signature" format="boolean" />
        <!-- file path for the digital signature keystore file -->
        <attr name="digital_signature_keystore_path" format="string" />
        <!-- password for the digital signature keystore file -->
        <attr name="digital_signature_keystore_password" format="string" />
        <!-- array of tools to disable -->
        <attr name="disable_tool_modes" format="reference" />
        <!-- array of annot types to disable editing -->
        <attr name="disable_annot_editing_by_types" format="reference" />
        <!-- whether to use pressure sensitive signatures -->
        <attr name="use_pressure_sensitive_signatures" format="boolean" />
        <!-- type of eraser -->
        <attr name="eraser_type" format="string" />
        <!-- whether to show undo/redo buttons in the toolbar-->
        <attr name="show_undo_redo" format="boolean" />
        <!-- whether to show rotate handle when annotation selected-->
        <attr name="show_rotate_handle" format="boolean" />
        <!-- whether to enable multi-stroke for ink tool-->
        <attr name="ink_multi_stroke_enabled" format="boolean" />
        <!--the margin between selection box and annotation bounding box in dp-->
        <attr name="selection_box_margin" format="integer" />
        <!--whether to show option to edit original PDF content-->
        <attr name="pdf_content_editing_enabled" format="integer" />
        <!--whether to restrict annotation creation on tap-->
        <attr name="restricted_tap_annot_creation" format="boolean" />
        <!--the half width of tap to create shape in dp-->
        <attr name="tap_to_create_half_width" format="integer" />
        <!--whether moving annotations between pages is allowed-->
        <attr name="move_annot_between_pages" format="boolean" />
        <!--sets whether free hand annotations are saved on a timer basis-->
        <attr name="free_hand_timer_enabled" format="boolean" />
        <!--sets how wide the range would be for auto smoothing vertical and horizontal strokes-->
        <attr name="free_highlighter_auto_smooth_range" format="float" />
    </declare-styleable>

